{
  "name": "maintainer-bot",
  "version": "1.0.0",
  "description": "A bot to automate and maintain your GitHub project!",
  "author": "Samuel Corsi-House <chouse.samuel@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/Xenfo/maintainer-bot.git",
  "homepage": "https://github.com/Xenfo/maintainer-bot",
  "bugs": "https://github.com/Xenfo/maintainer-bot/issues",
  "keywords": [
    "github",
    "bot",
    "automation"
  ],
  "main": "bin/app.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node bin/app.js",
    "start:dev": "nodemon bin/app.js",
    "single": "npm run build && node bin/commands/process-single.js",
    "many": "npm run build && node bin/commands/process-many.js",
    "single-info": "npm run build && node bin/commands/show-info.js",
    "single-info-debug": "npm run build && node --inspect-brk bin/commands/show-info.js",
    "update-test-data": "npm run build && node bin/commands/update-test-data.js",
    "cleanproject": "npm run build && node bin/commands/clean-columns.js",
    "create-fixture": "npm run build && node bin/commands/create-fixture.js",
    "update-all-fixtures": "npm run build && node bin/commands/update-all-fixtures.js",
    "graphql": "apollo client:codegen schema --target typescript --globalTypesFile=src/graphql/graphql-global-types.ts",
    "test": "jest",
    "install-cli": "npm run build && npm install -g ."
  },
  "dependencies": {
    "@apollo/client": "^3.3.18",
    "@octokit/rest": "^18.5.3",
    "@octokit/webhooks": "^9.6.1",
    "body-parser": "^1.19.0",
    "boxen": "^5.0.1",
    "chalk": "^4.1.1",
    "cli-boxes": "^3.0.0",
    "commander": "^7.2.0",
    "cross-env": "^7.0.3",
    "definitelytyped-header-parser": "github:Microsoft/definitelytyped-header-parser#production",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fast-json-patch": "^3.0.0-1",
    "graphql": "^15.5.0",
    "idembot": "^0.0.12",
    "moment": "^2.27.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "prettyjson": "^1.2.1",
    "react": "^17.0.2",
    "request": "^2.88.2",
    "shelljs": "^0.8.4",
    "smee-client": "^1.2.2",
    "typescript": "^4.2.4",
    "webport": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "26.0.23",
    "@types/localtunnel": "^2.0.0",
    "@types/node": "^15.3.0",
    "@types/node-fetch": "^2.5.10",
    "@types/prettyjson": "^0.0.29",
    "@types/request": "^2.48.5",
    "@types/shelljs": "^0.8.8",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.21.0",
    "apollo": "^2.33.2",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "26.6.3",
    "jest-file-snapshot": "^0.5.0",
    "localtunnel": "^2.0.1",
    "nock": "^13.0.11",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.6"
  },
  "engines": {
    "node": "> 12"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "bin": {
    "mtb": "bin/cli/mtb.js"
  }
}
